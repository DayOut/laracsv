{
    "name": "usmanhalalit/laracsv",
    "description": "A Laravel package to easily generate CSV files from Eloquent model.",
    "homepage": "https://github.com/usmanhalalit/laracsv",
    "keywords": ["laravel", "csv", "eloquent", "export"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors":
    [
        {
            "name": "Muhammad Usman",
            "email": "hi@usman.it",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.5.0",
        "league/csv": "8.2.*"
    },

    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "illuminate/database": "5.*@dev",
        "phpunit/phpunit": "5.7"
    },

    "autoload":	{
        "psr-4": {
            "Laracsv\\": "src/Laracsv/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Laracsv\\": "tests/Laracsv/"
        },

        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
